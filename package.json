{
  "name": "react-native-accessibility-engine",
  "version": "0.7.2",
  "description": "Make accessibility-related assertions on React Native code using React Test Renderer",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "react-native-accessibility-engine.podspec",
    "!lib/typescript/example",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/*.test.ts",
    "!**/*.test.tsx"
  ],
  "scripts": {
    "test": "jest",
    "typescript": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "prepare": "husky install && bob build",
    "release": "release-it",
    "example": "yarn --cwd example",
    "pods": "cd example && pod-install --quiet",
    "bootstrap": "yarn example && yarn && yarn pods"
  },
  "keywords": [
    "react-native",
    "jest",
    "testing",
    "testing-library",
    "a11y",
    "accessibility",
    "ios",
    "android"
  ],
  "repository": "https://github.com/aryella-lacerda/react-native-accessibility-engine",
  "author": "Aryella Lacerda <aryella.lacerda@gmail.com> (https://github.com/aryella-lacerda)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aryella-lacerda/react-native-accessibility-engine/issues"
  },
  "homepage": "https://github.com/aryella-lacerda/react-native-accessibility-engine#readme",
  "devDependencies": {
    "@commitlint/config-conventional": "15.0.0",
    "@react-native-community/bob": "0.17.1",
    "@react-native-community/eslint-config": "3.0.1",
    "@react-native-community/slider": "4.1.12",
    "@release-it/conventional-changelog": "3.3.0",
    "@testing-library/jest-native": "4.0.4",
    "@testing-library/react-native": "8.0.0",
    "@types/jest": "27.0.3",
    "@types/lodash.groupby": "4.6.6",
    "@types/react": "17.0.37",
    "@types/react-native": "0.66.8",
    "@types/react-test-renderer": "17.0.1",
    "babel-jest": "27.4.2",
    "commitlint": "15.0.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "7.0.4",
    "jest": "27.4.3",
    "pod-install": "0.1.29",
    "prettier": "2.5.1",
    "react": "17.0.2",
    "react-native": "0.66.3",
    "release-it": "14.11.8",
    "typescript": "4.5.2"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "jest": {
    "preset": "react-native",
    "moduleNameMapper": {
      "react-native-accessibility-engine": "<rootDir>/src/index",
      "tests/(.*)": "<rootDir>/src/__tests__/$1"
    },
    "modulePathIgnorePatterns": [
      "<rootDir>/src/__tests__/assets",
      "<rootDir>/example",
      "<rootDir>/node_modules",
      "<rootDir>/lib"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.ts"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": false
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  },
  "eslintConfig": {
    "extends": [
      "@react-native-community",
      "prettier"
    ],
    "globals": {
      "expect": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "quoteProps": "consistent",
          "singleQuote": true,
          "tabWidth": 2,
          "trailingComma": "es5",
          "useTabs": false
        }
      ]
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/",
    "coverage/"
  ],
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "@react-native-community/bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      "typescript"
    ]
  },
  "dependencies": {
    "lodash.groupby": "^4.6.0",
    "react-test-renderer": "^17.0.2"
  }
}
